{
  "name": "dashboard",
  "version": "1.0.0",
  "description": "Web-based OTLP realtime dashboard for Arrow IPC trace and metric visualization",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "npm run test:unit",
    "test:unit": "vitest run",
    "test:ui": "vitest --ui",
    "test:e2e": "playwright test",
    "format": "prettier --write \"**/*.{js,ts,jsx,tsx,css,scss,json,md,html,yaml,yml}\"",
    "format:check": "prettier --check \"**/*.{js,ts,jsx,tsx,css,scss,json,md,html,yaml,yml}\"",
    "lint": "eslint \"**/*.{js,ts,jsx,tsx}\" --max-warnings 0",
    "lint:fix": "eslint \"**/*.{js,ts,jsx,tsx}\" --fix"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@duckdb/duckdb-wasm": "^1.30.0",
    "apache-arrow": "^21.1.0",
    "plotly.js-dist-min": "^3.3.0",
    "vite": "^7.2.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@playwright/test": "^1.56.1",
    "@vitest/ui": "^4.0.13",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "playwright": "^1.56.1",
    "prettier": "^3.6.2",
    "vitest": "^4.0.13"
  }
}
